<form name="acteur_partie" method="post" action="/acteur/new" class="form-inline js-new-acteur-form" novalidate="novalidate" data-url="{{ path('acteur_partie_newJS')}}">

  <div id="acteur_partie">
    <div class="form-group">
      <label for="acteur_partie_nom" class="required">Nom</label>
      <input type="text" id="acteur_partie_nom" name="nom" required="required" maxlength="255" />
    </div>
    <div class="form-group">
      <label for="acteur_partie_nombreIndividus">Nombre individus</label>
      <input type="number" id="acteur_partie_nombreIndividus" name="nombreIndividus" />
    </div>
    <div class="form-group">
      <label for="acteur_partie_typeActeur" class="required">Type acteur</label>
      <select id="acteur_partie_typeActeur" name="typeActeur">
        <option value="4">Groupe d&#039;individus</option>
        <option value="6">Autorité Indépendante</option>
      </select>
    </div>
  </div>

<button class="btn">Save</button>

</form>
